{
  "compileOnSave": true,
  "compilerOptions": {
    "alwaysStrict": true,
    "baseUrl": "./public",
    "outDir": "./public/dist/",
    "diagnostics": true,
    "strict": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "module": "es6",
    "target": "es6",
    "allowJs": false
  },
  "exclude": [
    "node_modules"
  ]
}
